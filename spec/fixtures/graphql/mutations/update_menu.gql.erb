mutation {
  updateMenu(
  identifier: "<%= identifier %>"<%= ",\nlabel: \"#{label}\"" if label.present? %><%= ",\nstartDate: \"#{start_date}\"" if start_date.present? %><%= ",\nendDate: \"#{end_date}\"" if end_date.present? %><%= ",\nstate: \"#{state}\"" if state.present? %>
  ) {
    menu {
      label
      identifier
      state
      startDate
      endDate
    }
  }
}
